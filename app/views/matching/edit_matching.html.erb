<h1>수정 화면</h1>
<form action="/matching/update_matching/<%= @matching.id %>/" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  제목 : <input type="text" name="matching_title" value="<%= @matching.title %>"><br />
  내용 : <textarea name="matching_content" rows="8" cols="80"><%= @matching.content%></textarea><br />
  <input type="submit" value="제출">
</form>
